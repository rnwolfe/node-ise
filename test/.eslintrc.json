{
    "env": {
        "mocha": true
    },
    "extends": [
        "../.eslintrc.json",
        "plugin:mocha/recommended"
    ],
    "rules": {
        "global-require": 0,
        "implicit-arrow-linebreak": 0,
        "no-console": 0,
        "no-undef": 0,
        "no-unused-expressions": 0,
        "mocha/no-mocha-arrows": 0
    },
    "plugins": [
        "mocha"
    ]
}